    "babel-preset-es2015-webpack": "^6.4.3",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^1.9.0",
    "nodemon": "^1.11.0",
    "webpack": "^3.3.0",
    "winston": "^2.3.1"
  }
