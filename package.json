{
  "name": "gg-frontend",
  "version": "1.0.0",
  "description": "GG Frontend Panel",
  "main": "server/app.js",
  "scripts": {
    "start": "node ./server/bin/www",
    "start:dev": "nodemon ./server/bin/www",
    "start:stag": "NODE_ENV=staging && forever start ./server/bin/www",
    "start:prod": "NODE_ENV=production && forever start ./server/bin/www",
    "build:dev": "webpack --env.dev --watch",
    "build:stag": "webpack --env.stag",
    "build:prod": "webpack --env.prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hanu9/gg-frontend.git"
  },
  "author": "Hanuman Sahay Jat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hanu9/gg-frontend/issues"
  },
  "homepage": "https://github.com/hanu9/gg-frontend#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "forever": "^0.15.3",
    "html-minifier": "^3.5.2",
    "node-uuid": "^1.4.8",
    "nunjucks": "^3.0.1",
    "request": "^2.81.0",
    "winston-daily-rotate-file": "^1.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
